{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "experimentalDecorators": true,
    "baseUrl": "./",
    "paths": {
      "@src/*": [
        "./src/*"
      ],
      "@components": [
        "./src/components/*"
      ],
      "@screens": [
        "./src/screens/*"
      ],
      "@assets": [
        "./src/assets/*"
      ],
      "@hooks": [
        "./src/hooks/*"
      ],
      "@services": [
        "./src/services/*"
      ],
      "@styles": [
        "./src/styles/*"
      ]
    }
  }
}
